{
    "name": "nuxt-airbnb-ssr",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "prepare": "husky install",
        "dev:ssr": "cross-env NODE_ENV=development npx node-dev server.js",
        "prod:ssr": "cross-env NODE_ENV=production npx node-dev server.js",
        "build": "pnpm build:client && pnpm build:server",
        "build:client": "vite build --ssrManifest --outDir dist/client",
        "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
        "preview": "vite preview --host",
        "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
        "lint:style": "stylelint \"./**/*.{css,vue,html}\" --fix",
        "format": "prettier --write \"./**/*.{html,vue,ts,js,json,scss}\""
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "dependencies": {
        "@vueuse/core": "^9.1.0",
        "@vueuse/head": "^0.7.9",
        "axios": "^0.27.2",
        "element-plus": "^2.2.12",
        "express": "^4.18.1",
        "fs": "0.0.1-security",
        "pinia": "^2.0.18",
        "uuid": "^8.3.2",
        "vue": "^3.2.37",
        "vue-i18n": "^9.2.0",
        "vue-router": "^4.0.13"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^17.0.3",
        "@iconify-json/ep": "^1.1.6",
        "@intlify/vite-plugin-vue-i18n": "^3.4.0",
        "@tailwindcss/line-clamp": "^0.4.0",
        "@types/node": "^18.6.1",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vitejs/plugin-vue": "^3.0.0",
        "autoprefixer": "^10.4.8",
        "commitlint": "^17.0.3",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "esbuild-windows-64": "^0.14.51",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.3.0",
        "husky": "^8.0.1",
        "postcss": "^8.4.14",
        "postcss-html": "^1.5.0",
        "postcss-less": "^6.0.0",
        "prettier": "^2.7.1",
        "rollup-plugin-visualizer": "^5.7.1",
        "sass": "^1.54.0",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-less": "^1.0.4",
        "stylelint-config-standard": "^26.0.0",
        "stylelint-config-standard-vue": "^1.0.0",
        "stylelint-less": "^1.0.6",
        "stylelint-order": "^5.0.0",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.6.4",
        "unplugin-auto-import": "^0.10.3",
        "unplugin-icons": "^0.14.8",
        "unplugin-vue-components": "^0.21.2",
        "vite": "^3.0.0",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-inspect": "^0.5.0",
        "vite-plugin-svg-icons": "^2.0.1",
        "vite-svg-loader": "^3.4.0",
        "vue-eslint-parser": "^9.0.3",
        "vue-tsc": "^0.38.4"
    }
}
